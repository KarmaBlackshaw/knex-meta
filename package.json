{
  "name": "@jeash/knex-meta",
  "version": "1.2.9",
  "description": "Utility extensions for knex query builder",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "nodemon -r tsconfig-paths/register index.ts",
    "test": "vitest",
    "build": "tsup",
    "build:watch": "npm run build -- --watch src",
    "release": "release-it"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "knex",
    "extend knex"
  ],
  "author": "Ernie Jeash Villahermosa",
  "license": "ISC",
  "dependencies": {
    "@types/lodash": "^4.14.182",
    "bluebird": "^3.7.2",
    "knex": "^2.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "mysql": "^2.18.1",
    "tsup": "^6.1.3"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "7.32.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.3.1",
    "eslint-plugin-standard": "4.0.1",
    "jest": "^28.1.2",
    "nodemon": "^2.0.18",
    "release-it": "^15.2.0",
    "ts-node": "^10.8.2",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "vitest": "^0.17.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KarmaBlackshaw/knex-meta.git"
  },
  "bugs": {
    "url": "https://github.com/KarmaBlackshaw/knex-meta/issues"
  },
  "homepage": "https://github.com/KarmaBlackshaw/knex-meta#readme"
}
